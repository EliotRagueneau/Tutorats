def init ():
    L1=[]
    taille=eval(input('Indiquez la taille de la grille: '))
    for i in range(taille):
        L1.append(list(range(taille)))
        for x in range(taille):
            L1[i][x]=0
    a=eval(input("Indiquez position initiale x : "))
    b=eval(input("y: "))
    L1[a][b]=1
    return L1,a,b
    
    



def affigrille(L1):
    for i in range(len(L1)):
        print(L1[i])

def deplahaut(L1,a,b):
    L1[a][b]=0
    a=a-1
    b=b
    L1[a][b]=1
    return L1,a,b
def depladroite(L1,a,b):
    L1[a][b]=0
    a=a
    b=b+1
    L1[a][b]=1
    return L1,a,b

def deplabas(L1,a,b):
    L1[a][b]=0
    a=a+1
    b=b
    L1[a][b]=1
    return L1,a,b

def deplagauche(L1,a,b):
    L1[a][b]=0
    a=a
    b=b-1
    L1[a][b]=1
    return L1,a,b

    
    
